<div id="<%= dom_id workout %>">
  <p><span class="font-bold">Date:</span> <span> <%= time_ago_in_words workout.created_at %> ago</p>
  <p>
    <span class="font-bold">Name:</span>
    <%= workout.name %>
  </p>

  <p>
    <span class="font-bold">Description:</span>
    <%= workout.description %>
  </p>

  <p>
    <span class="font-bold">Muscle group:</span>
    <%= workout.muscle_group.name %>
  </p>
</div>
